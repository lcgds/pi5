{
  "name": "@govbr/dsgov",
  "version": "2.4.3",
  "description": "Design System do Governo Federal",
  "keywords": [
    "Design System GOV.BR",
    "componentes",
    "design",
    "system"
  ],
  "homepage": "https://gitcorporativo.serpro/dsgov-12314/dsgov.br/-/blob/develop/README.md",
  "bugs": "https://gitcorporativo.serpro/dsgov-12314/dsgov.br-backlogs/-/issues",
  "repository": {
    "type": "git",
    "url": "https://gitcorporativo.serpro:dsgov-12314/dsgov.br.git"
  },
  "license": "ISC",
  "author": "SERPRO/DIDES/SUPSE/SESEP",
  "files": [
    "dist",
    "src/**/*.scss"
  ],
  "scripts": {
    "build": "run-s build:webpack:both",
    "build:webpack:analyze-bundle": "run-s build:webpack:dev start-live-server",
    "build:webpack:both": "run-s clean:dist build:webpack:dev:no-clean build:webpack:prod:no-clean",
    "build:webpack:both:progress": "run-s clean:dist 'build:webpack:dev:no-clean -- --progress' 'build:webpack:prod:no-clean -- --progress'",
    "build:webpack:dev": "cross-env NODE_ENV=development webpack --mode development --progress",
    "build:webpack:dev:no-clean": "cross-env NODE_ENV=development webpack --mode development --env CLEAN=false",
    "build:webpack:dev:watch": "cross-env NODE_ENV=development webpack --mode development --env CLEAN=false --watch ",
    "build:webpack:prod": "cross-env NODE_ENV=production webpack --mode production",
    "build:webpack:prod:no-clean": "cross-env NODE_ENV=production webpack --mode production --env CLEAN=false",
    "clean:dist": "rimraf dist",
    "commit": "cz",
    "commit-release": "npm publish && npm run create:assets && git add . && git commit -m 'chore: cria e commita os assets da v'$npm_package_version'' ",
    "commit-assets": "git add . && git commit -m 'chore: cria e commita os assets da v'$npm_package_version''",
    "create:assets": "node ./createassets.js",
    "create:build": "node ./dsgov.util.js",
    "dev": "npm-run-all --parallel  build:webpack:dev:watch start-live-server",
    "lint": "run-s lint:*",
    "lint:html": "html-validate dist/**/*.html",
    "lint:js": "eslint -c .eslintrc.yaml --fix src/**/*.js",
    "lint:styles": "stylelint src/**/*.scss --fix",
    "live-server": "live-server",
    "minify:img": "node minify-img.js",
    "prepare": "husky install",
    "publish:design-review": "bash designreview.sh",
    "pushtags": "git push origin $(git describe --tags $(git rev-list --tags --max-count=1))",
    "release": "standard-version",
    "release:rc": "standard-version  --prerelease rc",
    "release:patch": "standard-version  --release-as patch --prerelease rc ",
    "start": "cross-env NODE_ENV=development webpack server --mode development --progress --port 9000 --host 0.0.0.0    ",
    "docs:sass": "sassdoc src/ --config .sassdocrc.yaml",
    "start-live-server": "live-server ./dist --port=9000 --host 0.0.0.0",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server"
  },
  "lint-staged": {
    "*.{css,scss}": "stylelint --fix",
    "*.{js,ts}": "eslint -c .eslintrc.yaml --fix"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "cz": "^1.8.2",
    "flatpickr": "^4.6.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@fiverr/afterbuild-webpack-plugin": "^1.0.0",
    "acorn": "^8.5.0",
    "autoprefixer": "^10.3.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.2.2",
    "babel-register": "^6.26.0",
    "branch-name-lint": "^2.1.1",
    "chalk": "^4.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.4",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "dir-archiver": "^1.2.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "focus-visible": "^5.2.0",
    "html-validate": "^5.1.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "imagemin": "^8.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-pngquant": "^9.0.2",
    "lint-staged": "^11.0.1",
    "live-server": "^1.2.1",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-get": "^0.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "^2.3.2",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.1",
    "sassdoc": "^2.7.3",
    "schema-utils": "^2.7.0",
    "shelljs": "^0.8.4",
    "standard-version": "^9.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.48.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "wget-improved": "^3.2.1",
    "yesno": "^0.3.1"
  },
  "engines": {
    "node": ">= 14.15.4",
    "npm": ">= 6.14.11",
    "yarn": ">= 1.22.5"
  },
  "engineStrict": true
}
